<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eaj.ufrn.mobilemilk">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:parentActivityName=".Activity.InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.InicioActivity"/>
        </activity>
        <activity
            android:name=".Activity.CadastrarActivity"
            android:parentActivityName=".Activity.InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.InicioActivity"/>
        </activity>
        <activity android:name=".Activity.HomeActivity">

        </activity>
        <activity
            android:name=".Activity.ComplementoCadastroClienteActivity"
            android:parentActivityName=".Activity.CadastrarActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CadastrarActivity"/>
        </activity>
        <activity
            android:name=".Activity.CadastrarFazendaActivity"
            android:parentActivityName=".Activity.ComplementoCadastroClienteActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.ComplementoCadastroClienteActivity"/>
        </activity>
        <activity
            android:name=".Activity.CadastrarAnalise"
            android:parentActivityName=".Activity.HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.HomeActivity"/>
        </activity>
    </application>

</manifest>